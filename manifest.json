{
    "manifest_version": 3,
    "name": "Tab Limiter",
    "version": "1.0",
    "description": "Blocks tabs from opening after a set limit of 10.",
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "action": {},
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "host_permissions": [
      "script-src 'self'; object-src 'self';"
    ],
    "sandbox": {
      "pages": [
        "popup.html"
      ],
      "allow-scripts": true,
      "allow-forms": true,
      "allow-popups": true,
      "allow-modals": true,
      "script-src": [
        "self",
        "unsafe-inline",
        "unsafe-eval"
      ],
      "child-src": [
        "self"
      ]
    },
    "browser_action": {
      "default_icon": "icon-48.png",
      "default_popup": "popup.html",
      "default_title": "Tab Limiter"
    }
  }
  
      

      