{
    "manifest_version": 2,
    "name": "Tab Limit Extension",
    "version": "1.0",
    "description": "Limits the number of tabs per window to 10",
    "permissions": [
      "tabs"
    ],
    "background": {
      "scripts": ["https://raw.githubusercontent.com/Mohav522/chrometrial/main/background.js"],
      "persistent": false
    },

    "content_security_policy": "script-src 'self' https://raw.githubusercontent.com/Mohav522/chrometrial/main/manifest.json; object-src 'self'",

  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  }
}
  