
{
    "manifest_version": 3,
    "name": "Tab Limiter",
    "version": "1.0",
    "icons": [
      {
        "src": "icon-16.png",
        "sizes": "16x16",
        "type": "image/png"
      },
      {
        "src": "icon-48.png",
        "sizes": "48x48",
        "type": "image/png"
      },
      {
        "src": "icon-128.png",
        "sizes": "128x128",
        "type": "image/png"
      }
    ],
    "host_permissions": [
      "https://github.com/Mohav522/chrometrial.git/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';",
      "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
    },
    "action": {},
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "browser_action": {
      "default_icon": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
      },
      "default_title": "Tab Limiter",
      "default_popup": "popup.html"
    }
  }
  