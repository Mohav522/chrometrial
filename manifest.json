
{
    "name": "Tab Limiter",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Blocks tabs from opening after a set limit of 10.",
    "content_security_policy": "script-src 'self' https://raw.githubusercontent.com; object-src 'self'",
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "activetabs"
    ],
    "background": {
        "scripts": ["chrometrial/background.js"]
        
      }
      
    }
    