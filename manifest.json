
{
    "manifest_version": 2,
    "name": "Tab Limiter",
    "version": "1.0",
    "content_security_policy": "script-src 'self' https://github.com/Mohav522/chrometrial.git; object-src 'self'",
    "description": "Blocks tabs from opening after a set limit of 10.",
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },

    "browser_action": {
        "default_icon": "icon-48.png",
        "default_popup": "popup.html",
        "default_title": "Tab Limiter"
    },

    "permissions": [
        "activeTab", 
        "tabs"
      ],

    "background": {
        "scripts": ["background.js"]
        
      }
      
    }
    